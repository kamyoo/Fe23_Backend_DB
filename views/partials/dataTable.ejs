<% if (dbData && dbData.length) { %>
    <table>
        <tr>
            <% Object.keys(dbData[0]).forEach(header => { %>
                <th><%= header %></th>
            <% }); %>
        </tr>
        <% dbData.forEach(row => { %>
            <tr>
                <% Object.values(row).forEach(value => { %>
                    <td><%= value %></td>
                <% }); %>
            </tr>
        <% }); %>
    </table>
<% } else { %>
    <p>No data available</p>
<% } %>
